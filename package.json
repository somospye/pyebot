{
  "name": "pyebot",
  "version": "1.0.0",
  "description": "Bot del Discord oficial de la comunidad Programadores y Estudiantes (PyE)",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:dev": "tsc --watch",
    "start": "tsc && tsc-alias && node dist/index.js",
    "dev": "tsc-watch --onSuccess \"node .\"",
    "fmt": "biome format --write",
    "fmt:check": "biome format",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "check": "biome check",
    "check:fix": "biome check --write"
  },
  "author": "Comunidad PyE",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "2.1.3",
    "@types/node": "^24.1.0",
    "@types/pg": "^8.15.5",
    "drizzle-kit": "^0.31.4",
    "lefthook": "^1.12.2",
    "lint-staged": "^16.1.2",
    "tsc-alias": "^1.8.16",
    "tsc-watch": "^7.1.1",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@google/genai": "^1.12.0",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.4",
    "ioredis": "^5.7.0",
    "module-alias": "^2.2.3",
    "pg": "^8.16.3",
    "seyfert": "^3.2.5",
    "sharp": "^0.34.3",
    "tesseract.js": "^6.0.1"
  },
  "lint-staged": {
    "*.{ts,js,mjs,cjs,json}": "biome check --write"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
